all: DOMINION RESETDOM

PFLAGS=-g -vb -dANSI

RESETDOM: RESETDOM.PAS Makefile
	fpc ${PFLAGS} RESETDOM.PAS

DOMINION: DOMINION.PAS COMMON.PAS OVERLAY1.DOM OVERLAY2.DOM OVERLAY3.DOM Makefile
	fpc ${PFLAGS} DOMINION.PAS

test: test.pas Makefile
	fpc ${PFLAGS} test.pas

clean:
	rm -fv DOMINION RESETDOM test *.o
